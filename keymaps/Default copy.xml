<keymap version="1" name="Default copy" parent="$default">
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="alt w" />
    <keyboard-shortcut first-keystroke="ctrl alt w" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl quote" />
  </action>
  <action id="Console.TableResult.NextPage" />
  <action id="Console.TableResult.PreviousPage" />
  <action id="EditorCloneCaretAbove">
    <keyboard-shortcut first-keystroke="ctrl alt up" />
  </action>
  <action id="EditorCloneCaretBelow">
    <keyboard-shortcut first-keystroke="ctrl alt down" />
  </action>
  <action id="Images.Editor.ToggleGrid" />
  <action id="Kotlin.RunScratch" />
  <action id="MoveElementLeft" />
  <action id="MoveElementRight" />
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="ctrl alt t" />
  </action>
  <action id="NextOccurence" />
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="shift ctrl alt right" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="shift ctrl alt left" />
  </action>
  <action id="PreviousOccurence" />
  <action id="ReformatWithPrettierAction">
    <keyboard-shortcut first-keystroke="shift ctrl alt p" />
